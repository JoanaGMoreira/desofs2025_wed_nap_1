              "privilegeLevel": "",
              "threats": [
                {
                  "id": "5b963b19-8f4d-4817-a712-42e197fdbba0",
                  "title": "Authentication Spoofing",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Spoofing",
                  "description": "An attacker spoofs a legitimate user by stealing their authentication token.",
                  "mitigation": "Implement 2FA with TOTP. Encrypt all data with TLS so that the token can't be stolen in-transit. Use tokens with short expiration windows.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 1,
                  "score": "9"
                },
                {
                  "id": "5ffed15c-3eb6-41cd-9213-f020b2ae7c28",
                  "title": "Information Tampering",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Tampering",
                  "description": "Attackers might try to tamper the information stored by the application, performing malicious requests in order to do so.",
                  "mitigation": "Use TLS to encrypt every connection. Validate all user-provided input according to well-defined rules. Implement queries with prepared statements to prevent tampering via database operations. ",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 2,
                  "score": "6"
                },
                {
                  "id": "62fd4796-cf9a-4a60-b3f3-784a41f65d8a",
                  "title": "Repudiation",
                  "status": "Mitigated",
                  "severity": "Low",
                  "type": "Repudiation",
                  "description": "Users might try to deny having performed certain actions on the platform.",
                  "mitigation": "Implement robust logging solutions to ensure traceability and accountability of all performed actions.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 3,
                  "score": "4"
                },
                {
                  "id": "87cd5975-1b2c-4d6e-bfe1-abc500368350",
                  "title": "Information Disclosure",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Information disclosure",
                  "description": "Attackers might try to release information which should not be accessible to the general public.",
                  "mitigation": "Validate all incoming requests. Encrypt data in transactions. Ensure that RBAC is strictly enforced so that access to data is allowed according to the least-privilege principle. Sanitize API responses to ensure no detailed error logs are sent to the client-side.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 4,
                  "score": "8"
                },
                {
                  "id": "5bb716e5-c592-463f-920a-edb534c3bebf",
                  "title": "DDoS",
                  "status": "Mitigated",
                  "severity": "High",
                  "type": "Denial of service",
                  "description": "An attacker might attempt to issue a flood of requests to the backend, rendering it unavailable to respond to legitimate requests",
                  "mitigation": "Implement rate limiting and request throttling measures to ensure that attackers can't flood the backend with requests.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 5,
                  "score": "12"
                },
                {
                  "id": "e2445d06-4dc6-410c-8bc9-021034b06cf5",
                  "title": "Elevation of Privilege",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Elevation of privilege",
                  "description": "A user might try to bypass access controls via insecure endpoints to elevate his privilege to an admin or park manager, thus becoming able to tamper with sensitive data. ",
                  "mitigation": "Enforce strict RBAC policies to ensure that users can't change their privileges. Re-validate the provided authentication token and match it with the operation being requested.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 6,
                  "score": "8"
                }
              ],
